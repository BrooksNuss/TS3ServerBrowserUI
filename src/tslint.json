{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      false,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      false,
      "element",
      "app",
      "kebab-case"
    ],
    "prefer-const": [
      false
    ],
    "align": false,
    "radix": false,
    "typedef": [
      true,
      "call-signature"
    ]
  }
}
